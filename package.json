{
  "name": "geccal_frontend",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@emotion/react": "^11.10.0",
    "@emotion/styled": "^11.10.0",
    "@mui/icons-material": "^5.8.4",
    "@mui/material": "^5.10.1",
    "@mui/x-data-grid": "^5.15.3",
    "@mui/x-date-pickers": "^6.18.7",
    "@reduxjs/toolkit": "^1.8.3",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/user-event": "^14.4.1",
    "@types/jest": "^27.5.2",
    "@types/node": "^17.0.45",
    "@types/react": "^18.0.15",
    "@types/react-dom": "^18.0.6",
    "dayjs": "^1.11.10",
    "notistack": "^2.0.5",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-redux": "^8.0.2",
    "react-router-dom": "^6.3.0",
    "react-scripts": "5.0.1",
    "typescript": "^4.7.4",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start": "npm run start:with-mock",
    "start:with-mock": "concurrently \"npm run start-server\" \"npm run start:standalone\"",
    "start:standalone": "react-scripts start --env standalone",
    "start-server": "json-server --watch mock-api/db.json --middlewares mock-api/middleware.js --port 3001 --delay 1000",
    "start:app": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "lint": "npx eslint . --ext .js,.jsx,.ts,.tsx",
    "update:snap": "npm run test -- -u "
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@testing-library/react": "^13.4.0",
    "concurrently": "^7.3.0",
    "json-server": "^0.17.0",
    "msw": "^0.47.3"
  }
}
